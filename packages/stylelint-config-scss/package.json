{
  "name": "@meludi/stylelint-config-scss",
  "version": "0.2.1",
  "description": "Shareable configuration for stylelint",
  "private": false,
  "license": "MIT",
  "keywords": [
    "stylelint",
    "stylelint-config",
    "postcss",
    "postcss-config"
  ],
  "author": {
    "email": "milad@al-ani.de",
    "name": "Milad Al-Ani"
  },
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./package.json": "./package.json",
    "./stylelint.config": "./stylelint.config.js",
    "./postcss.config": "./postcss.config.js"
  },
  "files": [
    "index.js",
    "stylelint.config.js",
    "postcss.config.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "precommit": "lint-staged",
    "lint": "eslint . --max-warnings 0",
    "test": "echo \"OK\" && exit 0"
  },
  "lint-staged": {
    "*.js": "npm run lint"
  },
  "dependencies": {
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.14",
    "postcss-html": "^1.0.0",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^7.4.1",
    "stylelint-config-idiomatic-css": "^1.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0"
  },
  "peerDependencies": {
    "stylelint": ">= 14 < 15"
  },
  "devDependencies": {
    "eslint": "^8.32.0",
    "lint-staged": "^13.1.0",
    "stylelint": "^14.16.1"
  },
  "engines": {
    "node": "^16.x || ^18.x"
  },
  "bugs": {
    "url": "https://github.com/meludi/stylelint-config/issues"
  },
  "homepage": "https://github.com/meludi/stylelint-config/tree/main/packages/stylelint-config-scss",
  "repository": {
    "type": "git",
    "url": "https://github.com/meludi/stylelint-config"
  }
}
